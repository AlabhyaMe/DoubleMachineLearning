mobile_bank <- sample(c(1,0),25000,prob=c(0.8,0.2),replace=T)
income <- as.integer((rnorm(25000,5000,2000)*rbinom(25000,1,0.1) +
rnorm(25000, 50000,20000) * rbinom(25000,1,0.1) +
rbinom(25000,1,0.8)*rnorm(25000,150000,30000)))
income<- ifelse(income<0,1,income)
saving_account <- rbinom(25000,1,0.9) * rnorm(25000,2000,300) +
rbinom(25000,1,0.8)*rnorm(25000,0.4,0.1)*income +
rnorm(25000,1000,400)
treated <- sample(c('Offer','No Offer'),25000, prob=c(0.7,0.3), replace=T)
cust_data <- tibble(
mobile_bank,income,saving_account,treated
)
CD_existing <- sample(0,25000,replace=T)
CD_existing <- ifelse(mobile_bank==1,
rbinom(25000,1,0.8)*(saving_account>quantile(saving_account,0.75))*rbinom(25000,1,0.8),
rbinom(25000,1,0.2)*(saving_account>quantile(saving_account,0.75))*rbinom(25000,1,0.7)
)
cust_data$mobile_bank_new <- ifelse(mobile_bank==1,
1,
rbinom(25000,1,p=0.15))
cust_data$CD_existing <- CD_existing
cust_data$CD_new <- ifelse(mobile_bank==1 & treated=="Offer",
rbinom(25000,1,0.8)*(saving_account>quantile(saving_account,0.5))*rbinom(25000,1,0.7),
rbinom(25000,1,0.2)*(saving_account>quantile(saving_account,0.5))*rbinom(25000,1,0.6)
)
sum(cust_data$CD_existing)
sum(cust_data$CD_new)
write.csv(cust_data, "cust_data.csv", row.names = FALSE)
